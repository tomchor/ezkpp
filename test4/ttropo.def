M   = 2.550E+19 ;
H2O    = 3.9E17 ;

OH     = 0.;
HO2    = 0.;
H2O2   = 0.;
H2O2aq = 0.;
HNO3   = 0.;
HNO3aq = 0.;
O1D = 0. ;
O   = 0. ;

CO     = 2.55E12; {100 ppbv}
ALL_SPEC = 0;


#INLINE F90_INIT
       TSTART = (12*3600)
       TEND = TSTART + (10*24*3600)
